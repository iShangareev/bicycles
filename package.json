{
  "name": "Bicycles",
  "version": "1.0.0",
  "description": "Стартовый шаблон сборки Лиги А",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "^10.2.5",
    "browser-sync": "2.26.x",
    "eslint": "4.16.0",
    "eslint-config-htmlacademy": "0.4.x",
    "gulp": "^4.0.2",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "^9.0.0",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.2.0",
